{
  "parser": "babel-eslint",
  "extends": [
  	"airbnb/legacy",
  	"eslint:recommended",
  	"plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
    }
  },
  "env": {
    "browser": true,
    "mocha": true,
    "node": true,
    "es6": true
  },
  "plugins": [
    "import",
    "react"
  ],
  "rules": {
    "react/prop-types": 0,
    "no-console":0,
    "no-unreachable": 0,
    "no-undef": 0
  }
}